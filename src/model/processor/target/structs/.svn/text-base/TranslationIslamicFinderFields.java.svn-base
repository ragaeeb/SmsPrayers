package model.processor.target.structs;

/**
 * Translates the parameter codes from the text message to the corresponding parameters for the
 * Islamic Finder website.
 *
 * @author rhaq
 * @version 1.00 2009-05-12 Initial submission.
 */
public class TranslationIslamicFinderFields extends IslamicFinderFields
{
	/** The requested month. */
	private String requestedMonth;

	/** The requested year. */
	private String requestedYear;


	/**
	 * Creates an instance of this class so that parameter codes may be translated to the
	 * Islamic Finder parameters.
	 */
	public TranslationIslamicFinderFields()
	{
		super();
	}


	/**
	 * Retrieves the requested month stored.
	 * @return The value of the requested month to calculate prayer times for.
	 */
	public String getRequestedMonth()
	{
		return requestedMonth;
	}


	/**
	 * Retrieves the value of the requested year stored.
	 * @return The value of the requested year to calculate prayer times for.
	 */
	public String getRequestedYear()
	{
		return requestedYear;
	}


	/**
	 * Sets the requested month to the specified value.
	 * @param requestedMonth The month to calculate prayer times for.
	 */
	public void setRequestedMonth(String requestedMonth)
	{
		this.requestedMonth = requestedMonth;
	}


	/**
	 * Sets the requested year to the specified value.
	 * @param requestedYear The year to calculate prayer times for.
	 */
	public void setRequestedYear(String requestedYear)
	{
		this.requestedYear = requestedYear;
	}
}